        <FreeForm enabled="true">
          <Label>Configuration of ThreeDModel</Label>
          <Bookmarks />
          <Breakpoints />
          <FreeFormScript><![CDATA[
//	//	Configuration of ThreeDModel

//	Setting Up of Objects
//		Note: ThreeDModel object is not available via the Object Browser
ThreeDModel tdmDS;		
ThreeDModel tdmSS;	
ThreeDModel tdmSSFP;	
ThreeDModel tdmSSRP;

StringArray TreeContentsDS;
StringArray TreeContentsSS;
StringArray TreeContentsSSFP;
StringArray TreeContentsSSRP;

Variable i;

String MainBody = "Main_Body";
String SolPanel = "Solar_Arrays";

Variable Scale = 100;

//	Importing of ThreeDModel
tdmDS.ModelFilename   = ThreeDModel_Filename;
tdmSS.ModelFilename   = ThreeDModel_Filename;
tdmSSFP.ModelFilename = ThreeDModel_Filename;
tdmSSRP.ModelFilename = ThreeDModel_Filename;

Console.CurrentTextColor = ColorTools.White;
Report @"~~~ ThreeDModel Selection ~~~\n" to Console;
Console.CurrentTextColor = ColorTools.Wheat;
Report @"\tThreeDModel Filename\t:" to Console;
Console.CurrentTextColor = ColorTools.White;
Report @"\t\t\t\t", ThreeDModel_Filename, @"\n" to Console;


//	Components of ThreeDModel
//		Note: Based on object name in Blender
tdmDS.ReportGroupTree(TreeContentsDS);
tdmSS.ReportGroupTree(TreeContentsSS);
tdmSSFP.ReportGroupTree(TreeContentsSSFP);
tdmSSRP.ReportGroupTree(TreeContentsSSRP);

TreeContentsDS.SortAscending();	
TreeContentsSS.SortAscending();	
TreeContentsSSFP.SortAscending();	
TreeContentsSSRP.SortAscending();	

Console.CurrentTextColor = ColorTools.Wheat;
Report @"\tConponents of SolarSat\t:" to Console;
Console.CurrentTextColor = ColorTools.White;
For i = 0 to TreeContentsDS.Dimension - 1;
	Report @"\t\t\t\t", TreeContentsDS[i] to Console;
End;
Report "" to Console;


//	Grouping of Main Body and Solar Array Components of the Spacecraft
//		Note: Based on number of objects and object naming in Blender
tdmDS.AddGroup(MainBody);
tdmSS.AddGroup(MainBody);
tdmSSFP.AddGroup(MainBody);
tdmSSRP.AddGroup(MainBody);

tdmDS.MoveGroup(MainBody,TreeContentsDS[0]);
tdmSS.MoveGroup(MainBody,TreeContentsSS[0]);
tdmSSFP.MoveGroup(MainBody,TreeContentsSSFP[0]);
tdmSSRP.MoveGroup(MainBody,TreeContentsSSRP[0]);

tdmDS.AddGroup(SolPanel);
tdmSS.AddGroup(SolPanel);
tdmSSFP.AddGroup(SolPanel);
tdmSSRP.AddGroup(SolPanel);

tdmDS.MoveGroup(SolPanel,TreeContentsDS[1]);
tdmSS.MoveGroup(SolPanel,TreeContentsSS[1]);
tdmSSFP.MoveGroup(SolPanel,TreeContentsSSFP[1]);
tdmSSRP.MoveGroup(SolPanel,TreeContentsSSRP[1]);

tdmDS.ReportGroupTree(TreeContentsDS);
tdmSS.ReportGroupTree(TreeContentsSS);
tdmSSFP.ReportGroupTree(TreeContentsSSFP);
tdmSSRP.ReportGroupTree(TreeContentsSSRP);

Console.CurrentTextColor = ColorTools.Wheat;
Report @"\tGrouping of Components\t:" to Console;
Console.CurrentTextColor = ColorTools.White;
For i = 0 to TreeContentsDS.Dimension - 1;
	Report @"\t\t\t\t", TreeContentsDS[i] to Console;
End;
Report "" to Console;
Report @"---------------------------------------------\n" to Console;


//	Set the ThreeDModel to be displayed on the Spacecraft
DummySat.SetThreeDModel(tdmDS);
SolarSat.SetThreeDModel(tdmSS);
SolarSatFP.SetThreeDModel(tdmSSFP);
SolarSatRP.SetThreeDModel(tdmSSRP);


//	Scale down the Model
DummySat.BodyScale   = Scale;
SolarSat.BodyScale   = Scale;
SolarSatFP.BodyScale = Scale;
SolarSatRP.BodyScale = Scale;

tdmDS.Scale   = Scale;
tdmSS.Scale   = Scale;
tdmSSFP.Scale = Scale;
tdmSSRP.Scale = Scale;


]]></FreeFormScript>
        </FreeForm>
        <FreeForm enabled="true">
          <Label>Configuration of Orbit and Spacecrafts</Label>
          <Bookmarks />
          <Breakpoints />
          <FreeFormScript><![CDATA[
//	//	Configuration of Orbit and Spacecrafts

//	Note: Using Spacecraft.OrbitWizardSunSyncSetup(Variable, TimeSpan, Variable, String, Variable) Method
//	First Variable set to 1 = Define SMA, Calculate Inclination. Evaluate MLT at Ascending Node

LoggingInfo.Show();
LoggingInfo.CurrentTextColor = ColorTools.White;
Report "" to LoggingInfo;
Report @"~~~ Orbital Selection ~~~\n" to LoggingInfo;


//	Epoch 
//		Conversion of epochs form calender-formatted string to numerical value
Epoch = epochString.ParseCalendarDate();	

LoggingInfo.CurrentTextColor = ColorTools.Crimson;
Report @"\tEpoch\t:" to LoggingInfo without linefeed;
LoggingInfo.CurrentTextColor = ColorTools.White;
Report @"\t", epochString to LoggingInfo;

LoggingInfo.CurrentTextColor = ColorTools.Crimson;
Report @"\tEpoch\t:" to LoggingInfo without linefeed;
LoggingInfo.CurrentTextColor = ColorTools.White;
Report @"\t", Epoch, @" days\n" to LoggingInfo;


//	Eccentricity
LoggingInfo.CurrentTextColor = ColorTools.Crimson;
Report @"\tEccentricity\t:" to LoggingInfo without linefeed;
LoggingInfo.CurrentTextColor = ColorTools.White;
Report @"\t", Eccentricity.Format("%1.2f"), @"\n" to LoggingInfo;


//	Mean Local Time of Ascending Node
LoggingInfo.CurrentTextColor = ColorTools.Crimson;
Report @"\tMLT of Ascending Node\t:" to LoggingInfo without linefeed;
LoggingInfo.CurrentTextColor = ColorTools.White;
Report @"\t", mltOfAscendingNode, @"\n" to LoggingInfo;


//	Semi-Major Axis (in km)
semiMajorAxis = Altitude + Earth.Radius;

LoggingInfo.CurrentTextColor = ColorTools.Crimson;
Report @"\tAltitude\t\t\t\t\t:" to LoggingInfo without linefeed;
LoggingInfo.CurrentTextColor = ColorTools.White;
Report @"\t", Altitude.Format("%3.1f"), " km" to LoggingInfo;

LoggingInfo.CurrentTextColor = ColorTools.Crimson;
Report @"\tSemi-Major Axis\t:" to LoggingInfo without linefeed;
LoggingInfo.CurrentTextColor = ColorTools.White;
Report @"\t", semiMajorAxis.Format("%4.3f"), " km" to LoggingInfo;




//	//	Configuration Spacecrafts for Sun-Synchronous Orbit
DummySat.OrbitWizardSunSyncSetup(1, Epoch, Eccentricity, mltOfAscendingNode, semiMajorAxis);
SolarSat.OrbitWizardSunSyncSetup(1, Epoch, Eccentricity, mltOfAscendingNode, semiMajorAxis);
SolarSatFP.OrbitWizardSunSyncSetup(1, Epoch, Eccentricity, mltOfAscendingNode, semiMajorAxis);
SolarSatRP.OrbitWizardSunSyncSetup(1, Epoch, Eccentricity, mltOfAscendingNode, semiMajorAxis);


//	Spacecraft Keplerian Coordinates
LoggingInfo.CurrentTextColor = ColorTools.White;
Report "" to LoggingInfo;
Report @"~~~ Keplerian Coordinates ~~~\n" to LoggingInfo;

LoggingInfo.CurrentTextColor = ColorTools.Crimson;
Report @"\tA\t\t\t:" to LoggingInfo without linefeed;
LoggingInfo.CurrentTextColor = ColorTools.White;
Report @"\t", DummySat.A.Format("%4.4f"), " km" to LoggingInfo;
//Report @"\t", SolarSat.A.Format("%4.4f"), " km" to LoggingInfo;
//Report @"\t", SolarSatFP.A.Format("%4.4f"), " km" to LoggingInfo;
//Report @"\t", SolarSatRP.A.Format("%4.4f"), " km" to LoggingInfo;

LoggingInfo.CurrentTextColor = ColorTools.Crimson;
Report @"\tE\t\t\t:" to LoggingInfo without linefeed;
LoggingInfo.CurrentTextColor = ColorTools.White;
Report @"\t", DummySat.E.Format("%1.2f") to LoggingInfo;
//Report @"\t", SolarSat.E.Format("%1.2f") to LoggingInfo;
//Report @"\t", SolarSatFP.E.Format("%1.2f") to LoggingInfo;
//Report @"\t", SolarSatRP.E.Format("%1.2f") to LoggingInfo;

LoggingInfo.CurrentTextColor = ColorTools.Crimson;
Report @"\tI\t\t\t:" to LoggingInfo without linefeed;
LoggingInfo.CurrentTextColor = ColorTools.White;
Report @"\t", DummySat.I.Format("%3.3f"), " deg" to LoggingInfo;
//Report @"\t", SolarSat.I.Format("%3.3f"), " deg" to LoggingInfo;
//Report @"\t", SolarSatFP.I.Format("%3.3f"), " deg" to LoggingInfo;
//Report @"\t", SolarSatRP.I.Format("%3.3f"), " deg" to LoggingInfo;

LoggingInfo.CurrentTextColor = ColorTools.Crimson;
Report @"\tRAAN\t:" to LoggingInfo without linefeed;
LoggingInfo.CurrentTextColor = ColorTools.White;
Report @"\t", DummySat.RAAN.Format("%3.3f"), " deg" to LoggingInfo;
//Report @"\t", SolarSat.RAAN.Format("%3.3f"), " deg" to LoggingInfo;
//Report @"\t", SolarSatFP.RAAN.Format("%3.3f"), " deg" to LoggingInfo;
//Report @"\t", SolarSatRP.RAAN.Format("%3.3f"), " deg" to LoggingInfo;

LoggingInfo.CurrentTextColor = ColorTools.Crimson;
Report @"\tW\t\t:" to LoggingInfo without linefeed;
LoggingInfo.CurrentTextColor = ColorTools.White;
Report @"\t", DummySat.W.Format("%3.3f"), " deg" to LoggingInfo;
