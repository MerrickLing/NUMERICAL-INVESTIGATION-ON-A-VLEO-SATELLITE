Report @"~~~ Keplerian Coordinates ~~~\n" to LoggingInfo;

LoggingInfo.CurrentTextColor = ColorTools.Crimson;
Report @"\tA\t\t\t:" to LoggingInfo without linefeed;
LoggingInfo.CurrentTextColor = ColorTools.White;
Report @"\t", DummySat.A.Format("%4.4f"), " km" to LoggingInfo;
//Report @"\t", SolarSat.A.Format("%4.4f"), " km" to LoggingInfo;
//Report @"\t", SolarSatFP.A.Format("%4.4f"), " km" to LoggingInfo;
