  <ProjectScript><![CDATA[
//	//	Mission Plan Description and Console Setup

//	Console Settings
Console.DockMode	= 3;
Console.Dimension 	= 36;
Console.BackColor 	= ColorTools.Black;

Console.CurrentFontType = 0;
Console.WordWrap 		= 1;

Console.WindowTitle	= "Solar Power Available for SSO";
Console.Show();


//	Report to Console: Paragraph 1
Console.CurrentTextColor = ColorTools.White;
Report "" to Console;
Report @"This Mission Plan calculates the " to Console without linefeed;
Console.CurrentTextColor = ColorTools.GreenYellow;
Report @"Solar Power " to Console without linefeed;
Console.CurrentTextColor = ColorTools.White;
Report @"available from a solar array which is fixed to the body coordinate system of a Spacecraft.\n" to Console;


//	Report to Console: Paragraph 2
Console.CurrentTextColor = ColorTools.GreenYellow;
Report @"Charging periods " to Console without linefeed;
Console.CurrentTextColor = ColorTools.White;
