Variable Az_pos = MaxTopArea;		//	(Z-axis) Max Top Area	 , in m^2 (Area includes solar panel arrays and solar panel on body)

Variable FrontPanelArea = TotalFrontPanelArea;	//	in m^2
Variable TopPanelArea 	= TotalTopPanelArea;	//	in m^2

//Variable FrontBodyArea = 0.3025;
//Variable TopBodyArea   = 0.6325;
//Variable ConnectorArea = 0.004; // Front or Top Projected Area

Variable newAx_pos_SSRP;
Variable newAz_pos_SSRP;


]]></FreeFormScript>
