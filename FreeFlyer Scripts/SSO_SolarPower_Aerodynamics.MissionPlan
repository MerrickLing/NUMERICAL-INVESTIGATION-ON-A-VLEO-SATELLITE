//	//	Drag Area Calculation Procedure: Area Vectors Method

Define Procedure DragAreaAV(Spacecraft sourceSC, Variable Ax_pos, Variable Ay_pos, Variable Az_pos, Variable newDragArea);

	//	//	Calculations for Drag Area

	//	Note 1: 	Euler Angles	=/=		Yaw, Roll and Pitch Angles
	//	Note 2:		For SlewManeuver Analysis, 
	//						Satellites are assigned MJ2000 Attitude Reference Frame,
	//						However,	Area Vectors Method performs calculations with respect to 
	//									LVLH-Earth Pointing Attitude Reference Frame,
	//						Therefore, .EulerAnglesLVLH[] is used instead of .EulerAngles[]

	
	//	Body Surface Areas
	//			Ax_pos: (X-axis) Max Frontal Area
	//			Ay_pos: (Y-axis) Max Side Area
	//			Az_pos: (Z-axis) Max Top Area	(Area includes solar panel arrays and solar panel on body)

	Variable Ax_neg = Ax_pos ;
	Variable Ay_neg = Ay_pos ;
	Variable Az_neg = Az_pos ;


	//	Body Coord System Unit Vectors
	Matrix Mx_pos = [ 1; 0; 0];
	Matrix My_pos = [ 0; 1; 0];
	Matrix Mz_pos = [ 0; 0; 1];

	Matrix Mx_neg = [-1; 0; 0];
	Matrix My_neg = [ 0;-1; 0];
	Matrix Mz_neg = [ 0; 0;-1];

	
	//	Body Reference Area Vectors
	Matrix MBx_pos = Ax_pos * Mx_pos ;
	Matrix MBy_pos = Ay_pos * My_pos ;
	Matrix MBz_pos = Az_pos * Mz_pos ;

	Matrix MBx_neg = Ax_neg * Mx_neg ;
	Matrix MBy_neg = Ay_neg * My_neg ;
	Matrix MBz_neg = Az_neg * Mz_neg ;
	

	//	Area Vectors in Navigational Refence Frame
	Matrix	MNx_pos;
	Matrix	MNy_pos;
	Matrix	MNz_pos;

	Matrix	MNx_neg;
	Matrix	MNy_neg;
	Matrix	MNz_neg;

	// Euler Angles in the LVLH Frame
	Variable phi;
	Variable theta;
	Variable psi;

	//	Direction Cosine Matrix and the Inverse (3-1-2 Transformation Sequence)
	Matrix dcm312;
	Matrix inv_dcm312;
	
	//	Projected Drag Area due to Individual Surfaces
	Variable x_pos_ProjArea;
	Variable y_pos_ProjArea;
	Variable z_pos_ProjArea;

	Variable x_neg_ProjArea;
	Variable y_neg_ProjArea;
	Variable z_neg_ProjArea;

	//	Assign Euler Angles
	phi		= sourceSC.EulerAnglesLVLH[0];			//	Asociated with Rotation about 3
	theta 	= sourceSC.EulerAnglesLVLH[1];			//	Asociated with Rotation about 1
	psi 	= sourceSC.EulerAnglesLVLH[2];			//	Asociated with Rotation about 2


	//	Calculate DCM	(Checked Formula)
	dcm312 =	[cos(rad(psi))*cos(rad(phi))-sin(rad(psi))*sin(rad(theta))*sin(rad(phi)),		cos(rad(psi))*sin(rad(phi))+cos(rad(phi))*sin(rad(psi))*sin(rad(theta)),		-cos(rad(theta))*sin(rad(psi)); 
				-cos(rad(theta))*sin(rad(phi)),												 	cos(rad(theta))*cos(rad(phi)), 													sin(rad(theta));
				sin(rad(psi))*cos(rad(phi))+cos(rad(psi))*sin(rad(theta))*sin(rad(phi)),	 	sin(rad(psi))*sin(rad(phi))-sin(rad(theta))*cos(rad(psi))*cos(rad(phi)), 		cos(rad(psi))*cos(rad(theta))	];
				

	//	Calculate Inverse DCM
	inv_dcm312 = dcm312.Inverse();


	//	Transforming Area Vectors from Body Reference Frame to Navigational Reference Frame
	MNx_pos = inv_dcm312 * MBx_pos;						//	'*' Operator: Matrix-Matrix dot product to give Matrix output
	MNy_pos = inv_dcm312 * MBy_pos;	
	MNz_pos = inv_dcm312 * MBz_pos;

	MNx_neg = inv_dcm312 * MBx_neg;
	MNy_neg = inv_dcm312 * MBy_neg;
	MNz_neg = inv_dcm312 * MBz_neg;


	//	Conversion of Area Vectors to Magnitudes, by Projecting Area Vectors onto the Unit X-axis Vector in Navigational Reference Frame
	x_pos_ProjArea = MNx_pos.DotProduct([1;0;0]);		//	.DotProduct(): Matrix-Matrix dot product to give Variable output
	y_pos_ProjArea = MNy_pos.DotProduct([1;0;0]);
	z_pos_ProjArea = MNz_pos.DotProduct([1;0;0]);

	x_neg_ProjArea = MNx_neg.DotProduct([1;0;0]);		//	If correct, x_pos_ProjArea = - x_neg_ProjArea	(Confirmed)
	y_neg_ProjArea = MNy_neg.DotProduct([1;0;0]);		//	If correct, y_pos_ProjArea = - y_neg_ProjArea	(Confirmed)
	z_neg_ProjArea = MNz_neg.DotProduct([1;0;0]);		//	If correct, z_pos_ProjArea = - z_neg_ProjArea	(Confirmed)


	//	Projected Drag Area
	//	Note:	Only positive areas are to be considered
	//			Since x_pos_ProjArea = - x_neg_ProjArea, similarly for y and z,
	//			Can simply find and add up the magnitude from the x, y, and z components
	newDragArea = sqrt((x_pos_ProjArea)^2) + sqrt((y_pos_ProjArea)^2) + sqrt((z_pos_ProjArea)^2) ;

EndProcedure; 




//	//	Drag Area Calculation Procedure: Position Velocity Method

Define Procedure DragAreaPV(Spacecraft sourceSC, Variable Ax_pos, Variable Ay_pos, Variable Az_pos, Variable relVelType,  Variable newDragArea);

	//	//	Calculations for Drag Area
	
	//	Array Form of Surface Areas
	Array BodyAreaBCS[3];
	BodyAreaBCS = {Ax_pos, Ay_pos, Az_pos};	
	
	
	//	Create Variables
	Variable index;	
	Variable VelocityRatio;
	Variable VelocityRatio_IRF;
	Variable VelocityRatio_RRF;

	
	//	Earth Angular Velocity Vector
	Array EarthRotation[3];
	Constant Variable EarthOmega = 7.2921158553e-005; // in rad/sec, Earth Rotation Rate (According to Markley)
	EarthRotation = {0, 0, EarthOmega};  			  // in rad/sec, Earth Rotation Rate about Earth's Axis of Rotation 

	
	//	Position and Velocity Vectors in MJ2000 Frame
	Array PosMJ2000[3];
	Array VelMJ2000[3];
	Array VelRot[3];
	
	Array PosMJ2000_IRF[3];
	Array VelMJ2000_IRF[3];
	Array EarthRotationCorrection_IRF[3];
	Array VelRot_IRF[3];

	Array PosMJ2000_RRF[3];
	Array VelMJ2000_RRF[3];
	Array PosECEF_RRF[3];
	Array EarthRotationCorrection_RRF[3];
	Array VelRot_RRF[3];
	

	//	Position and Velocity Vectors in BCS Frame
	Array PosBCS[3];
	Array VelBCS[3];
	Array UnitizedVelBCS[3];
	Array EffectiveArea[3];
	
	Array PosBCS_IRF[3];
	Array VelBCS_IRF[3];
	Array UnitizedVelBCS_IRF[3];
	Array EffectiveArea_IRF[3];

	Array PosBCS_RRF[3];
	Array VelBCS_RRF[3];
	Array UnitizedVelBCS_RRF[3];
	Array EffectiveArea_RRF[3];

	
	//	Initialize MJ2000 Position and Velocity Arrays
	PosMJ2000 = sourceSC.Position;
	VelMJ2000 = sourceSC.Velocity;
	
	PosMJ2000_IRF = sourceSC.Position;
	VelMJ2000_IRF = sourceSC.Velocity;
	
	PosMJ2000_RRF = sourceSC.Position;
	VelMJ2000_RRF = sourceSC.Velocity;
	
	
	//	Calculation of Relative Velocity
	If (relVelType == 0);
		//	TYPE I: No Atmospheric Velocity
		VelRot = VelMJ2000; 							//	Earth relative velocity
		VelocityRatio = (0 / VelMJ2000.Norm) * 100 ; 	//	For Comparison of Velocity Magnitudes
	
		PosBCS = AttitudeConvert(0, 3, sourceSC, PosMJ2000);
		VelBCS = AttitudeConvert(0, 3, sourceSC, VelRot);
		
		UnitizedVelBCS = VelBCS.Normalized(); 	// Unitize BCS Velocity vector
		For index = 0 to 2;
			EffectiveArea[index] = BodyAreaBCS[index] * UnitizedVelBCS[index];
		End;
		newDragArea = sqrt((EffectiveArea[0])^2) + sqrt((EffectiveArea[1])^2) + sqrt((EffectiveArea[2])^2);
		
		
	ElseIf (relVelType == 1);
		//	TYPE II: Atmospheric Velocity - Inertial Reference Frame
		EarthRotationCorrection_IRF = EarthRotation.CrossProduct(PosMJ2000_IRF); 				//	Earth Rotation Correction
		VelRot_IRF = VelMJ2000_IRF - EarthRotationCorrection_IRF; 								//	Earth relative velocity
		VelocityRatio_IRF = (EarthRotationCorrection_IRF.Norm / VelMJ2000_IRF.Norm) * 100 ; 	//	For Comparison of Velocity Magnitudes
		
		PosBCS_IRF = AttitudeConvert(0, 3, sourceSC, PosMJ2000_IRF);
		VelBCS_IRF = AttitudeConvert(0, 3, sourceSC, VelRot_IRF);
		
		UnitizedVelBCS_IRF = VelBCS_IRF.Normalized(); 	// Unitize BCS Velocity vector
		For index = 0 to 2;
			EffectiveArea_IRF[index] = BodyAreaBCS[index] * UnitizedVelBCS_IRF[index];
		End;
		newDragArea = sqrt((EffectiveArea_IRF[0])^2) + sqrt((EffectiveArea_IRF[1])^2) + sqrt((EffectiveArea_IRF[2])^2);
		
		
	End;

EndProcedure; 



