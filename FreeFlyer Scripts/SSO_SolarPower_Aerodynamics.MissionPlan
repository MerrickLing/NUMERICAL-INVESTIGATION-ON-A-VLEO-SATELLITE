		Step DummySat;
	End;
End;


//	For SolarSat
If (ErrCount_SS == 0);
	Try sending ErrorCount to ErrCount_SS;
		Step SolarSat to (SolarSat.Epoch == DummySat.Epoch);
	End;
End;


//	For SolarSatFP
If (ErrCount_SSFP == 0);
	Try sending ErrorCount to ErrCount_SSFP;
		Step SolarSatFP to (SolarSatFP.Epoch == DummySat.Epoch);
	End;
End;


//	For SolarSatRP
If (ErrCount_SSRP == 0);
	Try sending ErrorCount to ErrCount_SSRP;
		Step SolarSatRP to (SolarSatRP.Epoch == DummySat.Epoch);
	End;
End;


//	Stop the Simulation Analysis once all Spacecraft reaches Earth
If (ErrCount_DS != 0 and ErrCount_SS != 0 and ErrCount_SSFP != 0 and ErrCount_SSRP != 0);
	Stop;
End;


]]></FreeFormScript>
