Report @"\tMLT of Ascending Node\t:" to LoggingInfo without linefeed;
LoggingInfo.CurrentTextColor = ColorTools.White;
Report @"\t", mltOfAscendingNode, @"\n" to LoggingInfo;


//	Semi-Major Axis (in km)
semiMajorAxis = Altitude + Earth.Radius;

LoggingInfo.CurrentTextColor = ColorTools.Crimson;
