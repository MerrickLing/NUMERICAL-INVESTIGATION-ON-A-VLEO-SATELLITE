        <FreeForm enabled="true">
          <Label>Calculation of Spacecrafts Pitch Angles (between X-Axis &amp; Vel Vector)</Label>
          <Bookmarks />
          <Breakpoints />
          <FreeFormScript><![CDATA[
//	//	Calculation of Spacecrafts Pitch Angles (between X-Axis and Velocity Vector)

// For DummySat
pitchAngle_DS = DummySat_X.VertexAngle(DummySat_Vel);

DummySat_X_Array   = AttitudeConvert(0, 2, DummySat, DummySat_X.Element);
DummySat_Vel_Array = AttitudeConvert(0, 2, DummySat, DummySat_Vel.Element);

DummySat_X_Array   = DummySat_X_Array.Normalized();
DummySat_Vel_Array = DummySat_Vel_Array.Normalized();

If (DummySat_X_Array[2] > DummySat_Vel_Array[2]);
	pitchAngle_DS = -pitchAngle_DS;
Else;
	pitchAngle_DS =  pitchAngle_DS;
End;


//	For SolarSat
pitchAngle_SS = SolarSat_X.VertexAngle(SolarSat_Vel);

SolarSat_X_Array   = AttitudeConvert(0, 2, SolarSat, SolarSat_X.Element);
SolarSat_Vel_Array = AttitudeConvert(0, 2, SolarSat, SolarSat_Vel.Element);

SolarSat_X_Array   = SolarSat_X_Array.Normalized();
SolarSat_Vel_Array = SolarSat_Vel_Array.Normalized();

pitchAngle_SS = abs(pitchAngle_SS);


//	For SolarSatFP
pitchAngle_SSFP = SolarSatFP_X.VertexAngle(SolarSatFP_Vel);

SolarSatFP_X_Array   = AttitudeConvert(0, 2, SolarSatFP, SolarSatFP_X.Element);
SolarSatFP_Vel_Array = AttitudeConvert(0, 2, SolarSatFP, SolarSatFP_Vel.Element);

SolarSatFP_X_Array   = SolarSatFP_X_Array.Normalized();
SolarSatFP_Vel_Array = SolarSatFP_Vel_Array.Normalized();

If (SolarSatFP_X_Array[2] > SolarSatFP_Vel_Array[2]);
	pitchAngle_SSFP = -pitchAngle_SSFP;
Else;
	pitchAngle_SSFP =  pitchAngle_SSFP;
End;


//	For SolarSatRP
pitchAngle_SSRP = SolarSatRP_X.VertexAngle(SolarSatRP_Vel);

SolarSatRP_X_Array   = AttitudeConvert(0, 2, SolarSatRP, SolarSatRP_X.Element);
SolarSatRP_Vel_Array = AttitudeConvert(0, 2, SolarSatRP, SolarSatRP_Vel.Element);

SolarSatRP_X_Array   = SolarSatRP_X_Array.Normalized();
SolarSatRP_Vel_Array = SolarSatRP_Vel_Array.Normalized();

If (SolarSatRP_X_Array[2] > SolarSatRP_Vel_Array[2]);
	pitchAngle_SSRP_base = -pitchAngle_SSRP;
Else;
	pitchAngle_SSRP_base =  pitchAngle_SSRP;
End;

If (SARotation >= -180 and SARotation <=90);
	pitchAngle_SSRP = SARotation + pitchAngle_SSRP_base;
ElseIf(SARotation >= -270 and SARotation <-180);
	pitchAngle_SSRP = 360 + SARotation + pitchAngle_SSRP_base;
End;	


]]></FreeFormScript>
        </FreeForm>
        <FreeForm enabled="true">
          <Label>Solar Panel Attitude, Power &amp; Shadow Calculations</Label>
          <Bookmarks />
          <Breakpoints />
          <FreeFormScript><![CDATA[
//	//	Panel Orientation/Attitude Convert

//	Transform the sun vector to BCS using AttitudeConvert
SunBCS_DS = AttitudeConvert(0, 3, DummySat, DummySat.VectorToObject(Sun));
SunBCS_SS = AttitudeConvert(0, 3, SolarSat, SolarSat.VectorToObject(Sun));
SunBCS_SSFP = AttitudeConvert(0, 3, SolarSatFP, SolarSatFP.VectorToObject(Sun));
SunBCS_SSRP = AttitudeConvert(0, 3, SolarSatRP, SolarSatRP.VectorToObject(Sun));


//	Solar Panel orientation, Vector Normal to the Solar Array, aligned to Spacecraft BCS
PanelVecBCS = {0, 0, 1};
PanelVecBCS_SSRP = {sin(rad(SARotation)), 0, cos(rad(SARotation))};



//	//	Panel Power & Shadow Calculations

//	For DummySat
If (DummySat.InShadow == 0);
	//	Calculate the Angle between Sun Vector and Panel Normal
	sunPanelAngle_DS = PanelVecBCS.VertexAngle(SunBCS_DS);

	// Calculate power from solar panel
	panelPower_DS = Panel_Efficiency * Js * Panel_Area * abs(cos(rad(sunPanelAngle_DS)));
	
ElseIf (DummySat.InShadow == 1);
	panelPower_DS  = 0;
	
End;

Shadow_DS = DummySat.InShadow;
If (ShadowPrev_DS != Shadow_DS);
    LoggingInfo.CurrentTextColor = ColorTools.Goldenrod;
	If (Shadow_DS == 1);
		Report "DS: Entering Shadow", DummySat.Epoch.ConvertToCalendarDate("Mmm DD hh:mm:ss") to LoggingInfo;
	Else;
		Report "DS: Leaving Shadow", DummySat.Epoch.ConvertToCalendarDate("Mmm DD hh:mm:ss") to LoggingInfo;
	End;
	ShadowPrev_DS = Shadow_DS;
End;


//	For SolarSat
If (SolarSat.InShadow == 0);
	//	Calculate the Angle between Sun Vector and Panel Normal
	sunPanelAngle_SS = PanelVecBCS.VertexAngle(SunBCS_SS);

	// Calculate power from solar panel
	panelPower_SS = Panel_Efficiency * Js * Panel_Area * abs(cos(rad(sunPanelAngle_SS)));

ElseIf (SolarSat.InShadow == 1);
	panelPower_SS  = 0;
	
End;

Shadow_SS = SolarSat.InShadow;
If (ShadowPrev_SS != Shadow_SS);
    LoggingInfo.CurrentTextColor = ColorTools.Goldenrod;
	If (Shadow_SS == 1);
		Report "SS: Entering Shadow", SolarSat.Epoch.ConvertToCalendarDate("Mmm DD hh:mm:ss") to LoggingInfo;
	Else;
		Report "SS: Leaving Shadow", SolarSat.Epoch.ConvertToCalendarDate("Mmm DD hh:mm:ss") to LoggingInfo;
	End;
	ShadowPrev_SS = Shadow_SS;
End;


//	For SolarSatFP
If (SolarSatFP.InShadow == 0);
	//	Calculate the Angle between Sun Vector and Panel Normal
	sunPanelAngle_SSFP = PanelVecBCS.VertexAngle(SunBCS_SSFP);

	// Calculate power from solar panel
	panelPower_SSFP = Panel_Efficiency * Js * Panel_Area * abs(cos(rad(sunPanelAngle_SSFP)));
	
ElseIf (SolarSatFP.InShadow == 1);
	panelPower_SSFP = 0;
	
End;

Shadow_SSFP = SolarSatFP.InShadow;
If (ShadowPrev_SSFP != Shadow_SSFP);
    LoggingInfo.CurrentTextColor = ColorTools.Goldenrod;
	If (Shadow_SSFP == 1);
		Report "SSFP: Entering Shadow", SolarSatFP.Epoch.ConvertToCalendarDate("Mmm DD hh:mm:ss") to LoggingInfo;
	Else;
		Report "SSFP: Leaving Shadow", SolarSatFP.Epoch.ConvertToCalendarDate("Mmm DD hh:mm:ss") to LoggingInfo;
	End;
	ShadowPrev_SSFP = Shadow_SSFP;
End;


//	For SolarSatRP
If (SolarSatRP.InShadow == 0);
	//	Calculate the Angle between Sun Vector and Panel Normal
	sunPanelAngle_SSRP = PanelVecBCS_SSRP.VertexAngle(SunBCS_SSRP);

	// Calculate power from solar panel
	panelPower_SSRP = Panel_Efficiency * Js * Panel_Area * abs(cos(rad(sunPanelAngle_SSRP)));
	
ElseIf (SolarSatRP.InShadow == 1);
	panelPower_SSRP = 0;
	
End;

Shadow_SSRP = SolarSatRP.InShadow;
If (ShadowPrev_SSRP != Shadow_SSRP);
    LoggingInfo.CurrentTextColor = ColorTools.Goldenrod;
	If (Shadow_SSRP == 1);
		Report "SSRP: Entering Shadow", SolarSatRP.Epoch.ConvertToCalendarDate("Mmm DD hh:mm:ss") to LoggingInfo;
	Else;
		Report "SSRP: Leaving Shadow", SolarSatRP.Epoch.ConvertToCalendarDate("Mmm DD hh:mm:ss") to LoggingInfo;
	End;
	ShadowPrev_SSRP = Shadow_SSRP;
End;


]]></FreeFormScript>
        </FreeForm>
        <ScriptRegion enabled="true" collapsed="false" StartLineComment="Calculation of Drag Related Properties">
          <FreeForm enabled="true">
            <Label>Calculation of Coefficient of Drag</Label>
            <Bookmarks />
            <Breakpoints />
            <FreeFormScript><![CDATA[
//	//	Calculation of Coefficient of Drag

//	For DummySat
If (pitchAngle_DS < -90 or pitchAngle_DS > 90);
	//	Adjusting Angles to Fit Within Formula Region (-90 to 90 Deg)
	If (pitchAngle_DS >= -180 and pitchAngle_DS < -90);
		x_DS = pitchAngle_DS + 180;	// in Deg
	ElseIf (pitchAngle_DS > 90 and pitchAngle_DS <= 180);
		x_DS = pitchAngle_DS - 180;	// in Deg
	End;

Else;
	x_DS = pitchAngle_DS;	// in Deg
	
End;

y_DS = DummySat.Height;	// in km
	
If (x_DS >= -90 and x_DS < -20);
	newCd_DS = 2.0154 + 0.001178*x_DS + 0.00051258*y_DS + 3.6151*10^-05*x_DS^2 + -8.7829*10^-07*x_DS*y_DS + -1.4424*10^-06*y_DS^2 + 2.0506*10^-07*x_DS^3 + -1.7234*10^-08*x_DS^2*y_DS + -1.31*10^-08*x_DS*y_DS^2 + 2.254*10^-09*y_DS^3;

ElseIf (x_DS >= -20 and x_DS < -5);
	newCd_DS = 2.1637 + 0.084126*x_DS + 0.0035953*y_DS + 0.0076318*x_DS^2 + 0.00024868*x_DS*y_DS + -4.9498*10^-06*y_DS^2 + 0.00019388*x_DS^3 + 5.4827*10^-06*x_DS^2*y_DS + -1.2219*10^-07*x_DS*y_DS^2 + 4.1113*10^-09*y_DS^3;

ElseIf (x_DS >= -5 and x_DS <= 5);
	newCd_DS = 2.647 + 0.0018265*x_DS + 0.0095079*y_DS + -0.049269*x_DS^2 + 4.4563*10^-06*x_DS*y_DS + -1.9707*10^-05*y_DS^2 + -7.9434*10^-05*x_DS^2*y_DS + -2.673*10^-08*x_DS*y_DS^2 + 1.8586*10^-08*y_DS^3;

ElseIf (x_DS > 5 and x_DS <= 20);
	newCd_DS = 2.295 + -0.09462*x_DS + 0.0028119*y_DS + 0.0080744*x_DS^2 + -0.0002318*x_DS*y_DS + -2.7391*10^-06*y_DS^2 + -0.00020452*x_DS^3 + 5.9214*10^-06*x_DS^2*y_DS + 8.8481*10^-08*x_DS*y_DS^2 + 1.8565*10^-09*y_DS^3;
	
ElseIf (x_DS > 20 and x_DS <= 90);
	newCd_DS = 2.0069 + -0.00076481*x_DS + 0.00054999*y_DS + 2.9335*10^-05*x_DS^2 + 5.2195*10^-07*x_DS*y_DS + -1.3788*10^-06*y_DS^2 + -1.6473*10^-07*x_DS^3 + -2.5639*10^-08*x_DS^2*y_DS + 1.5737*10^-08*x_DS*y_DS^2 + 1.8705*10^-09*y_DS^3;

End;
DummySat.Cd = newCd_DS;	//	Assign new Cd value



//	For SolarSat
If (pitchAngle_SS < -90 or pitchAngle_SS > 90);
	//	Adjusting Angles to Fit Within Formula Region (-90 to 90 Deg)
	If (pitchAngle_SS >= -180 and pitchAngle_SS < -90);
		x_SS = pitchAngle_SS + 180;	// in Deg
	ElseIf (pitchAngle_SS > 90 and pitchAngle_SS <= 180);
		x_SS = pitchAngle_SS - 180;	// in Deg
	End;

Else;
	x_SS = pitchAngle_SS;	// in Deg
	
End;

y_SS = SolarSat.Height;	// in km
	
If (x_SS >= -90 and x_SS < -20);
	newCd_SS = 2.0154 + 0.001178*x_SS + 0.00051258*y_SS + 3.6151*10^-05*x_SS^2 + -8.7829*10^-07*x_SS*y_SS + -1.4424*10^-06*y_SS^2 + 2.0506*10^-07*x_SS^3 + -1.7234*10^-08*x_SS^2*y_SS + -1.31*10^-08*x_SS*y_SS^2 + 2.254*10^-09*y_SS^3;

ElseIf (x_SS >= -20 and x_SS < -5);
	newCd_SS = 2.1637 + 0.084126*x_SS + 0.0035953*y_SS + 0.0076318*x_SS^2 + 0.00024868*x_SS*y_SS + -4.9498*10^-06*y_SS^2 + 0.00019388*x_SS^3 + 5.4827*10^-06*x_SS^2*y_SS + -1.2219*10^-07*x_SS*y_SS^2 + 4.1113*10^-09*y_SS^3;

ElseIf (x_SS >= -5 and x_SS <= 5);
	newCd_SS = 2.647 + 0.0018265*x_SS + 0.0095079*y_SS + -0.049269*x_SS^2 + 4.4563*10^-06*x_SS*y_SS + -1.9707*10^-05*y_SS^2 + -7.9434*10^-05*x_SS^2*y_SS + -2.673*10^-08*x_SS*y_SS^2 + 1.8586*10^-08*y_SS^3;

ElseIf (x_SS > 5 and x_SS <= 20);
	newCd_SS = 2.295 + -0.09462*x_SS + 0.0028119*y_SS + 0.0080744*x_SS^2 + -0.0002318*x_SS*y_SS + -2.7391*10^-06*y_SS^2 + -0.00020452*x_SS^3 + 5.9214*10^-06*x_SS^2*y_SS + 8.8481*10^-08*x_SS*y_SS^2 + 1.8565*10^-09*y_SS^3;
	
ElseIf (x_SS > 20 and x_SS <= 90);
	newCd_SS = 2.0069 + -0.00076481*x_SS + 0.00054999*y_SS + 2.9335*10^-05*x_SS^2 + 5.2195*10^-07*x_SS*y_SS + -1.3788*10^-06*y_SS^2 + -1.6473*10^-07*x_SS^3 + -2.5639*10^-08*x_SS^2*y_SS + 1.5737*10^-08*x_SS*y_SS^2 + 1.8705*10^-09*y_SS^3;

End;
SolarSat.Cd = newCd_SS;	//	Assign new Cd value



//	For SolarSatFP
If (pitchAngle_SSFP < -90 or pitchAngle_SSFP > 90);
	//	Adjusting Angles to Fit Within Formula Region (-90 to 90 Deg)
	If (pitchAngle_SSFP >= -180 and pitchAngle_SSFP < -90);
		x_SSFP = pitchAngle_SSFP + 180;	// in Deg
	ElseIf (pitchAngle_SSFP > 90 and pitchAngle_SSFP <= 180);
		x_SSFP = pitchAngle_SSFP - 180;	// in Deg
	End;

Else;
	x_SSFP = pitchAngle_SSFP;	// in Deg
	
End;

y_SSFP = SolarSatFP.Height;	// in km
	
If (x_SSFP >= -90 and x_SSFP < -20);
	newCd_SSFP = 2.0154 + 0.001178*x_SSFP + 0.00051258*y_SSFP + 3.6151*10^-05*x_SSFP^2 + -8.7829*10^-07*x_SSFP*y_SSFP + -1.4424*10^-06*y_SSFP^2 + 2.0506*10^-07*x_SSFP^3 + -1.7234*10^-08*x_SSFP^2*y_SSFP + -1.31*10^-08*x_SSFP*y_SSFP^2 + 2.254*10^-09*y_SSFP^3;

ElseIf (x_SSFP >= -20 and x_SSFP < -5);
	newCd_SSFP = 2.1637 + 0.084126*x_SSFP + 0.0035953*y_SSFP + 0.0076318*x_SSFP^2 + 0.00024868*x_SSFP*y_SSFP + -4.9498*10^-06*y_SSFP^2 + 0.00019388*x_SSFP^3 + 5.4827*10^-06*x_SSFP^2*y_SSFP + -1.2219*10^-07*x_SSFP*y_SSFP^2 + 4.1113*10^-09*y_SSFP^3;

ElseIf (x_SSFP >= -5 and x_SSFP <= 5);
	newCd_SSFP = 2.647 + 0.0018265*x_SSFP + 0.0095079*y_SSFP + -0.049269*x_SSFP^2 + 4.4563*10^-06*x_SSFP*y_SSFP + -1.9707*10^-05*y_SSFP^2 + -7.9434*10^-05*x_SSFP^2*y_SSFP + -2.673*10^-08*x_SSFP*y_SSFP^2 + 1.8586*10^-08*y_SSFP^3;

ElseIf (x_SSFP > 5 and x_SSFP <= 20);
	newCd_SSFP = 2.295 + -0.09462*x_SSFP + 0.0028119*y_SSFP + 0.0080744*x_SSFP^2 + -0.0002318*x_SSFP*y_SSFP + -2.7391*10^-06*y_SSFP^2 + -0.00020452*x_SSFP^3 + 5.9214*10^-06*x_SSFP^2*y_SSFP + 8.8481*10^-08*x_SSFP*y_SSFP^2 + 1.8565*10^-09*y_SSFP^3;
	
ElseIf (x_SSFP > 20 and x_SSFP <= 90);
	newCd_SSFP = 2.0069 + -0.00076481*x_SSFP + 0.00054999*y_SSFP + 2.9335*10^-05*x_SSFP^2 + 5.2195*10^-07*x_SSFP*y_SSFP + -1.3788*10^-06*y_SSFP^2 + -1.6473*10^-07*x_SSFP^3 + -2.5639*10^-08*x_SSFP^2*y_SSFP + 1.5737*10^-08*x_SSFP*y_SSFP^2 + 1.8705*10^-09*y_SSFP^3;

End;
SolarSatFP.Cd = newCd_SSFP;	//	Assign new Cd value



//	For SolarSatRP
If (pitchAngle_SSRP < -90 or pitchAngle_SSRP > 90);
	//	Adjusting Angles to Fit Within Formula Region (-90 to 90 Deg)
	If (pitchAngle_SSRP >= -180 and pitchAngle_SSRP < -90);
		x_SSRP = pitchAngle_SSRP + 180;	// in Deg
	ElseIf (pitchAngle_SSRP > 90 and pitchAngle_SSRP <= 180);
		x_SSRP = pitchAngle_SSRP - 180;	// in Deg
	End;
	
Else;
	x_SSRP = pitchAngle_SSRP;	// in Deg
	
End;

y_SSRP = SolarSatRP.Height;	// in km

If (x_SSRP >= -90 and x_SSRP < -20);
	newCd_SSRP = 2.0498 + 0.0039155*x_SSRP + 0.0011522*y_SSRP + 7.1702*10^-05*x_SSRP^2 + -1.6988*10^-06*x_SSRP*y_SSRP + -3.5479*10^-06*y_SSRP^2 + 3.7969*10^-07*x_SSRP^3 + -3.4152*10^-08*x_SSRP^2*y_SSRP + -1.3766*10^-08*x_SSRP*y_SSRP^2 + 4.53*10^-09*y_SSRP^3;
	
ElseIf (x_SSRP >= -20 and x_SSRP < -5);
	newCd_SSRP = 2.202 + 0.098636*x_SSRP + 0.0047588*y_SSRP + 0.0084267*x_SSRP^2 + 0.00023956*x_SSRP*y_SSRP + -8.7602*10^-06*y_SSRP^2 + 0.00020863*x_SSRP^3 + 4.9199*10^-06*x_SSRP^2*y_SSRP + -1.3025*10^-07*x_SSRP*y_SSRP^2 + 8.1251*10^-09*y_SSRP^3;
	
ElseIf (x_SSRP >= -5 and x_SSRP <= 5);
	newCd_SSRP = 2.5923 + -0.00024171*x_SSRP + 0.010133*y_SSRP + -0.046752*x_SSRP^2 + 5.567*10^-06*x_SSRP*y_SSRP + -2.1924*10^-05*y_SSRP^2 + -7.2934*10^-05*x_SSRP^2*y_SSRP + -5.6302*10^-09*x_SSRP*y_SSRP^2 + 2.1053*10^-08*y_SSRP^3;
	
ElseIf (x_SSRP > 5 and x_SSRP <= 20);
	newCd_SSRP = 2.3068 + -0.10741*x_SSRP + 0.0039833*y_SSRP + 0.0089939*x_SSRP^2 + -0.00024049*x_SSRP*y_SSRP + -5.9729*10^-06*y_SSRP^2 + -0.00021914*x_SSRP^3 + 4.9476*10^-06*x_SSRP^2*y_SSRP + 1.2306*10^-07*x_SSRP*y_SSRP^2 + 5.1709*10^-09*y_SSRP^3;
	
ElseIf (x_SSRP > 20 and x_SSRP <= 90);
	newCd_SSRP = 2.0465 + -0.0022796*x_SSRP + 0.00096743*y_SSRP + 3.5876*10^-05*x_SSRP^2 + 2.6137*10^-06*x_SSRP*y_SSRP + -2.993*10^-06*y_SSRP^2 + -1.2183*10^-07*x_SSRP^3 + -6.6393*10^-08*x_SSRP^2*y_SSRP + 1.789*10^-08*x_SSRP*y_SSRP^2 + 3.7736*10^-09*y_SSRP^3;
	
End;
SolarSatRP.Cd = newCd_SSRP;	//	Assign new Cd value


]]></FreeFormScript>
          </FreeForm>
          <FreeForm enabled="true">
            <Label>Calculations of Drag Area: Area Vectors Method</Label>
            <Bookmarks />
            <Breakpoints />
            <FreeFormScript><![CDATA[
//	//	Calculations of Drag Area: Area Vectors Method

//	For DummySat
Call DragAreaAV(DummySat, Ax_pos, Ay_pos, Az_pos, newDragArea_DS);
DummySat.DragArea = newDragArea_DS;

//	For SolarSat
Call DragAreaAV(SolarSat, Ax_pos, Ay_pos, Az_pos, newDragArea_SS);
SolarSat.DragArea = newDragArea_SS;

//	For SolarSatFP
Call DragAreaAV(SolarSatFP, Ax_pos, Ay_pos, Az_pos, newDragArea_SSFP);
SolarSatFP.DragArea = newDragArea_SSFP;

//	For SolarSatRP
Call DragAreaAV(SolarSatRP, Ax_pos, Ay_pos, Az_pos, newDragArea_SSRP);
newDragArea_SSRP = newDragArea_SSRP - sqrt((FrontPanelArea * (cos(rad(pitchAngle_SSRP_base))))^2) - sqrt((TopPanelArea * (cos(rad(pitchAngle_SSRP_base+90))))^2);
newDragArea_SSRP = newDragArea_SSRP + sqrt((FrontPanelArea * (cos(rad(pitchAngle_SSRP))))^2) + sqrt((TopPanelArea * (cos(rad(pitchAngle_SSRP+90))))^2);
SolarSatRP.DragArea = newDragArea_SSRP;


]]></FreeFormScript>
          </FreeForm>
          <FreeForm enabled="false" comment="Allows accounting of atmospheric velocity">
            <Label>Calculations of Drag Area: Position Velocity Method</Label>
            <Bookmarks />
            <Breakpoints />
            <FreeFormScript><![CDATA[
//	//	Calculations of Drag Area: Position Velocity Method

//	For DummySat
Call DragAreaPV(DummySat, Ax_pos, Ay_pos, Az_pos, 1, newDragArea_DS);
DummySat.DragArea = newDragArea_DS;

//	For SolarSat
Call DragAreaPV(SolarSat, Ax_pos, Ay_pos, Az_pos, 1, newDragArea_SS);
SolarSat.DragArea = newDragArea_SS;

//	For SolarSatFP
Call DragAreaPV(SolarSatFP, Ax_pos, Ay_pos, Az_pos, 1, newDragArea_SSFP);
SolarSatFP.DragArea = newDragArea_SSFP;

//	For SolarSatRP
//Call DragAreaPV(SolarSatRP, Ax_pos, Ay_pos, Az_pos, 0, newDragArea_SSRP);
//newDragArea_SSRP = newDragArea_SSRP - sqrt((FrontPanelArea * (cos(rad(pitchAngle_SSRP_base))))^2) - sqrt((TopPanelArea * (cos(rad(pitchAngle_SSRP_base+90))))^2);
//newDragArea_SSRP = newDragArea_SSRP + sqrt((FrontPanelArea * (cos(rad(pitchAngle_SSRP))))^2) + sqrt((TopPanelArea * (cos(rad(pitchAngle_SSRP+90))))^2);

newAx_pos_SSRP = Ax_pos - FrontPanelArea;
newAx_pos_SSRP = newAx_pos_SSRP + sqrt((FrontPanelArea * (cos(rad(pitchAngle_SSRP-pitchAngle_SSRP_base))))^2) + sqrt((TopPanelArea * (cos(rad(pitchAngle_SSRP-pitchAngle_SSRP_base+90))))^2);

newAz_pos_SSRP = Az_pos - TopPanelArea;  
newAz_pos_SSRP = newAz_pos_SSRP	+ sqrt((FrontPanelArea * (cos(rad(pitchAngle_SSRP-pitchAngle_SSRP_base+90))))^2) + sqrt((TopPanelArea * (cos(rad(pitchAngle_SSRP-pitchAngle_SSRP_base))))^2);

Call DragAreaPV(SolarSatRP, newAx_pos_SSRP, Ay_pos, newAz_pos_SSRP, 1, newDragArea_SSRP);
SolarSatRP.DragArea = newDragArea_SSRP;


]]></FreeFormScript>
          </FreeForm>
          <FreeForm enabled="true">
            <Label>Calculation of Drag Force Magnitude</Label>
            <Bookmarks />
            <Breakpoints />
            <FreeFormScript><![CDATA[
//	//	Calculation of Drag Force Magnitude

//	For DummySat 
//DragForce_DS = sqrt((DummySat.Drag[0])^2 + (DummySat.Drag[1])^2 + (DummySat.Drag[2])^2);	
DragForce_DS = DummySat.Drag.Norm;


//	For SolarSat 
//DragForce_SS = sqrt((SolarSat.Drag[0])^2 + (SolarSat.Drag[1])^2 + (SolarSat.Drag[2])^2);	
DragForce_SS = SolarSat.Drag.Norm;
		

//	For SolarSatFP
//DragForce_SSFP = sqrt((SolarSatFP.Drag[0])^2 + (SolarSatFP.Drag[1])^2 + (SolarSatFP.Drag[2])^2);	
DragForce_SSFP = SolarSatFP.Drag.Norm;


//	For SolarSatRP
//DragForce_SSRP = sqrt((SolarSatRP.Drag[0])^2 + (SolarSatRP.Drag[1])^2 + (SolarSatRP.Drag[2])^2);	
DragForce_SSRP = SolarSatRP.Drag.Norm;


]]></FreeFormScript>
          </FreeForm>
        </ScriptRegion>
        <FreeForm enabled="true">
          <Label>Simulation Stop for Short Analysis</Label>
          <Bookmarks />
          <Breakpoints />
          <FreeFormScript><![CDATA[
//	//	Simulation Stop for Short Analysis

If (AnalysisType == 0.0);
	If (DummySat_OrbitCounter > OrbitToStop and SolarSat_OrbitCounter > OrbitToStop and SolarSatFP_OrbitCounter > OrbitToStop and SolarSatRP_OrbitCounter > OrbitToStop);
		Stop;	
	End;
End;


]]></FreeFormScript>
        </FreeForm>
        <FreeForm enabled="true">
          <Label>Report to External Files</Label>
          <Bookmarks />
          <Breakpoints />
          <FreeFormScript><![CDATA[
//	//	Report to External Files

If (AnalysisType == 0.0);
	
	//	For Attitude
	Report DummySat.ElapsedTime, 
				pitchAngle_DS, pitchAngle_SS, pitchAngle_SSFP, pitchAngle_SSRP	to AttitudeReport;
	
	//	For Coefficient of Drag
	Report DummySat.ElapsedTime, 
				newCd_DS, newCd_SS, newCd_SSFP, newCd_SSRP	to CdReport;
	
	//	For Drag Area
	Report DummySat.ElapsedTime, 
				newDragArea_DS, newDragArea_SS, newDragArea_SSFP, newDragArea_SSRP to DragAreaReport;
	
	//	For Drag Force
