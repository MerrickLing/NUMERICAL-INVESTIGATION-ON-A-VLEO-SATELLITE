Vector LVLH_Z;
LVLH_Z.BuildVector(9, SolarSat, Earth);					//	LVLH Z-Positive Vector (Definition: Negative Position Vector)
LVLH_Z.Epoch = SolarSat.Epoch;

Vector OrbitNormal;
Vector LVLH_Y;
OrbitNormal.BuildVector(5, SolarSat);					//	LVLH Y-Positive Vector (Definition: Negative Orbit Normal Vector)
LVLH_Y.BuildVector(10, OrbitNormal);					// 		5 = Body Angular Momentum,	10 = Anti-Vector
OrbitNormal.Epoch = SolarSat.Epoch;						
LVLH_Y.Epoch = SolarSat.Epoch;

CoordinateSystem csToEarth;
csToEarth.BuildCoordinateSystem(3, LVLH_Z, 2, LVLH_Y);	//	Building of LVLH Coordinate System
csToEarth.SetPositionVector(EarthSc);
csToEarth.VisualScale = 250;


//	SunPointing Coordinate System 
Vector SunSc;
SunSc.BuildVector(9, Sun, SolarSat);					//	Anti-Z Vector
SunSc.Epoch = SolarSat.Epoch;

CoordinateSystem csToSun;
//csToSun.BuildCoordinateSystem(3, ScSun, 2, ScEarth);	//	Building of SunPointing Coordinate System 
														//		(Error case 1: Satellite positive Z-axis is aligned to ScSun Contact Vector. Need to either pitch or roll 180deg)
														
//csToSun.BuildCoordinateSystem(3, SunSc, 2, ScEarth);	//	Building of SunPointing Coordinate System 
														//		(X-axis faces Leftward first then Rightward. Pitch 180deg from Error case 1, Satellite negative Z-axis is aligned to ScSun Contact Vector.)

csToSun.BuildCoordinateSystem(3, SunSc, 2, EarthSc);	//	Building of SunPointing Coordinate System 
														//		(X-axis faces Rightward first then Leftward. Roll 180deg from Error case 1, Satellite negative Z-axis is aligned to ScSun Contact Vector.)

