//	//	Configuration Spacecrafts for Sun-Synchronous Orbit
DummySat.OrbitWizardSunSyncSetup(1, Epoch, Eccentricity, mltOfAscendingNode, semiMajorAxis);
SolarSat.OrbitWizardSunSyncSetup(1, Epoch, Eccentricity, mltOfAscendingNode, semiMajorAxis);
SolarSatFP.OrbitWizardSunSyncSetup(1, Epoch, Eccentricity, mltOfAscendingNode, semiMajorAxis);
