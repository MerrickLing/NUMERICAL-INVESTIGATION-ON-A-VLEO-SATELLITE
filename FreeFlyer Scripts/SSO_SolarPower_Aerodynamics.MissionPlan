      <ScriptRegion enabled="true" collapsed="false" StartLineComment="Slew Manoeuvre Procedure">
        <FreeForm enabled="true">
          <Label>ComputeTargetAttitude Procedure</Label>
          <Bookmarks />
          <Breakpoints />
          <FreeFormScript><![CDATA[
//	//	ComputeTargetAttitude Procedure

Define Procedure ComputeTargetAttitude(Spacecraft sourceSC, GroundStation targetGS, CoordinateSystem targetCS, Variable targetType);

	//	This Procedure builds the targetCS CoordinateSystem based on the inputs
	//	Note: For targetTypes, 0 = LVLH (EarthPointing) Coordinate System 
	//						   1 = SunPointing Coordinate System
	//						   2 = GroundStation-Pointing Coordinate System

	//	Create Vectors
	Vector EarthSc;
	
	Vector LVLH_Z;
	Vector OrbitNormal;
	Vector LVLH_Y;
	
	Vector scEarth;
	Vector velocityVector;
	
	Vector SunSc;
	
	Vector scGrndStn;
	
	Array 	SunLVLHArray[3];
	Array 	SunProjLVLHArray[3];
	Vector 	SunProjLVLHVector;
	Array 	SolarArray[3];
	Vector 	SolarVector;

	
	//	Synchronize epochs of Vector and CoordinateSystem objects with the sourceSC Spacecraft
	EarthSc.Epoch 	= sourceSC.Epoch;
	
	LVLH_Z.Epoch 	  = sourceSC.Epoch;
	OrbitNormal.Epoch = sourceSC.Epoch;	
	LVLH_Y.Epoch 	  = sourceSC.Epoch;
	
	scEarth.Epoch 		 = sourceSC.Epoch;
	velocityVector.Epoch = sourceSC.Epoch;
	
	SunSc.Epoch 	= sourceSC.Epoch;
	
	scGrndStn.Epoch = sourceSC.Epoch;

	SolarVector.Epoch = sourceSC.Epoch;
	
	targetCS.Epoch  = sourceSC.Epoch;

	
	//	Defining Positional Vector
	EarthSc.BuildVector(9, Earth, sourceSC);
	
	
	//	Building of targetCS Coordinate Systems based on Target Type
	If (targetType == 0); 		
		//	LVLH Coordinate System
		LVLH_Z.BuildVector(9, sourceSC, Earth);	
		OrbitNormal.BuildVector(5, sourceSC);	
		LVLH_Y.BuildVector(10, OrbitNormal);	
		targetCS.BuildCoordinateSystem(3, LVLH_Z, 2, LVLH_Y);
		
//		scEarth.BuildVector(9, sourceSC, Earth);			
//		velocityVector.BuildVector(7, sourceSC);
//		targetCS.BuildCoordinateSystem(3, scEarth, 1, velocityVector);
		
		targetCS.SetPositionVector(EarthSc);
		

	ElseIf (targetType == 1);	
		//	SunPointing Coordinate System 
		scEarth.BuildVector(9, sourceSC, Earth);
		SunSc.BuildVector(9, Sun, sourceSC);
		targetCS.BuildCoordinateSystem(3, SunSc, 2, scEarth);
		targetCS.SetPositionVector(EarthSc);
		
		
	ElseIf (targetType == 2);
		//	GrndStnPointing Coordinate System 
		velocityVector.BuildVector(7, sourceSC);
		scGrndStn.BuildVector(9, sourceSC, targetGS);
		targetCS.BuildCoordinateSystem(3, scGrndStn, 1, velocityVector);
		targetCS.SetPositionVector(EarthSc);
		
		
	ElseIf (targetType == 3);
		//	SunPointing Coordinate System: Fixed Panels
		SunSc.BuildVector(9, Sun, sourceSC);							//	For Alignment with Anti-Z Axis
		SunLVLHArray = AttitudeConvert(0, 2, sourceSC, SunSc.Element);	//	From MJ2000 to LVLH
		
		SunProjLVLHArray = {SunLVLHArray[0], 0, SunLVLHArray[2]};		//	Determine Vector Projection in LVLH
		SunProjLVLHVector.Element = SunProjLVLHArray;					//	Array to Vector
		
		SolarArray = AttitudeConvert(2, 0, sourceSC, SunProjLVLHVector.Element);	//	From LVLH to MJ2000
		SolarVector.Element = SolarArray;											//	Array to Vector
		
		OrbitNormal.BuildVector(5,sourceSC);
		LVLH_Y.BuildVector(10,OrbitNormal);
		
		targetCS.BuildCoordinateSystem(3, SolarVector, 2, LVLH_Y);
		targetCS.SetPositionVector(EarthSc);
		
		
	End;

EndProcedure; 


]]></FreeFormScript>
        </FreeForm>
        <FreeForm enabled="true">
          <Label>SlewSpacecraft Procedure</Label>
          <Bookmarks />
          <Breakpoints />
          <FreeFormScript><![CDATA[
//	//	SlewSpacecraft Procedure

Define Procedure SlewSpacecraft(CoordinateSystem targetCS, Variable performSlew, Variable slewRate, Spacecraft sc);

	//	This Procedure slews the Spacecraft orientation toward the targetCS CoordinateSystem, constrained by the slewRate

	//	Create Objects
	Variable slewAngle;
	Variable availableAngle;
	Array slewAxis[3];
	Array deltaQuat[4];

	CoordinateSystem prevCS;
	CoordinateSystem deltaCS;
	CoordinateSystem newCS;
	
	Variable ErrCount = 0;

	//	Synchronize Epochs of CoordinateSystem objects with the sc Spacecraft
	prevCS.Epoch   = sc.Epoch;
	targetCS.Epoch = sc.Epoch;
	deltaCS.Epoch  = sc.Epoch;
	newCS.Epoch    = sc.Epoch;

	//	If the performSlew input Variable is false, then the Spacecraft will jump immediately to the targetCS
	If (performSlew == 1);
		//	Build the prevCS CoordinateSystem by retrieving the orientation of the sc Spacecraft
		prevCS.BuildCoordinateSystem(sc);

		//	Compute the difference between the prevCS and targetCS coordinate systems
		//	Note:
		//		target = delta  * previous
		//		delta  = target * previous.Inverse()
		deltaCS.SetAttitudeMatrix(targetCS.GetAttitudeMatrix()*prevCS.GetAttitudeMatrix().Inverse());

		//	Compute delta quaternion in order to get the slewAngle and slewAxis
		deltaQuat = deltaCS.GetQuaternion();

		//	Compute the slewAngle and slewAxis using the delta quaternion 
		slewAngle = 2*acos(deltaQuat[3]);
		
		Try sending ErrorCount to ErrCount;
			slewAxis = {deltaQuat[0], deltaQuat[1], deltaQuat[2]}.Normalized();
		End;
		If (ErrCount != 0);
			sc.SetOrientation(targetCS);
			ErrCount = 0;
			ExitProcedure;
		End;

		//	Compute the largest possible angle that the spacecraft can slew in one time step based on the slewRate and step size
		availableAngle = rad(slewRate*sc.Propagator.StepSize.ToSeconds);

		//	Scale the delta quaternion down while keeping the axis of rotation the same if the total slew angle is larger than the available angle  
		If (slewAngle >= availableAngle);

			deltaQuat[3] = cos(availableAngle/2);
			deltaQuat[0:2] = slewAxis*sqrt(1-deltaQuat[3]^2);

			deltaCS.SetQuaternion(deltaQuat);
		End;

		//	Compute the new Body Coordinate System of the Spacecraft
		//	Note:
		//      new = delta * previous
		newCS.SetAttitudeMatrix(deltaCS.GetAttitudeMatrix()*prevCS.GetAttitudeMatrix());

		//	Apply CoordinateSystem update to sc
		sc.SetOrientation(newCS);
		
	Else;
		//	Apply the targetCS CoordinateSystem to the sc Spacecraft
		sc.SetOrientation(targetCS);
		
	End;

EndProcedure; 


]]></FreeFormScript>
        </FreeForm>
        <FreeForm enabled="true">
          <Label>SlewPanels Procedure</Label>
          <Bookmarks />
          <Breakpoints />
          <FreeFormScript><![CDATA[
//	//	SlewPanels Procedure

Define Procedure SlewPanels(Variable slewType, Spacecraft sourceSC, Variable SARotation, Array Quaternions);

	//	This Procedure slews the Solar Panels orientation toward the Sun

	//	Create Objects
	Vector   SunVector;
	Array	 SunBCSArray[3];
	CoordinateSystem cs;

	
	//	Synchronize Epochs of CoordinateSystem objects with the sourceSC Spacecraft
	SunVector.Epoch = sourceSC.Epoch;
	

	//	Build vector from sourceSC to the Sun
	SunVector.BuildVector(9, sourceSC, Sun);
	
	//	Convert vector to BCS 
	SunBCSArray = AttitudeConvert(0, 3, sourceSC, SunVector.Element);

	//	Calculate SA rotation
	If (slewType == 1);
		//	Get angle for SA rotation, in X-Z plane
		SARotation = deg(atan2(SunBCSArray[2], SunBCSArray[0]));
		SARotation = -90 - SARotation;
		
	ElseIf (slewType == 0);
		//	Get angle for SA rotation, in X-Z plane
		SARotation = 0;
		
	End;
	
	//	Obtain Quaternions
	cs.SetEulerAngles(0, 0, SARotation, "3-1-2");
	Quaternions = cs.GetQuaternion();

EndProcedure; 


]]></FreeFormScript>
        </FreeForm>
