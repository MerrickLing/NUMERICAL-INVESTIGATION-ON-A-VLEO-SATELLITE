//	//	Satellite Description: SolarSat and DummySat

//	SolarSat Description
//		-	Main Satellite used to analyse the various scenarios.
//		-	Attitude Reference Frame varies according to the selected scenario.
//		-	Assigned Color: Fuchsia 
//	DummySat Description
//		-	Reference Satellite used for checking, verifying, calculations, etc.
//		-	Attitude Reference Frame is fixed at LVLH - Earth Pointing.
//		-	Assigned Color: Lime

//	Report to Console: Paragraph 3
Report @"During these charging periods, the Spacecraft will be analysed for 4 different scenarios:\n" to Console;


//	Report to Console: Paragraph 3, List 1
Console.CurrentTextColor = ColorTools.Aqua;
Report @"\t1) Non-SunTracking (Fixed Attitude)" to Console;

Report @"\t\ta) " to Console without linefeed;
Console.CurrentTextColor = ColorTools.White;
Report @"Ram Facing, NO Offset" to Console;
Report @"\t\t\t\t(i.e., LVLH CS)\n" to Console;


//	Report to Console: Paragraph 3, List 2
Console.CurrentTextColor = ColorTools.Gold;
Report @"\t2) SunTracking (Variable Attitude)" to Console;

Report @"\t\ta) " to Console without linefeed;
Console.CurrentTextColor = ColorTools.White;
Report @"3-DOF" to Console;
Report @"\t\t\t\t(i.e., SunPointing CS)" to Console;

Console.CurrentTextColor = ColorTools.Gold;
Report @"\t\tb) " to Console without linefeed;
Console.CurrentTextColor = ColorTools.White;
Report @"1-DOF, Fixed Panels" to Console;
Report @"\t\t\t\t(i.e., Pitching Only)" to Console;

Console.CurrentTextColor = ColorTools.Gold;
Report @"\t\tc) " to Console without linefeed;
Console.CurrentTextColor = ColorTools.White;
Report @"1-DOF, Rotating Panels" to Console;
Report @"\t\t\t\t(i.e., Pitching Only)\n" to Console;


//	Report to Console: Paragraph 4
Report @"The Suntracking scenarios undergo slew maneuver to maximize charging surface area.\n" to Console;
Report @"---------------------------------------------\n" to Console;




//	//	Coordinate System Description

//	Report to Console: Paragraph 5
Console.CurrentTextColor = 16744192;
Report "Local-Vertical-Local-Horizonal (LVLH) Coordinate System" to Console;
Console.CurrentTextColor = ColorTools.White;
Report @"Default Spacecraft reference frame.\n" to Console;

Report @"---------------------------------------------\n" to Console;
Console.CurrentTextColor = 32767;
Report "SunPointing Coordinate System" to Console;
Console.CurrentTextColor = ColorTools.White;
Report @"Orients the Spacecraft to constantly points towards the Sun.\n" to Console;

Report @"---------------------------------------------\n" to Console;
Console.CurrentTextColor = 16711935;
Report "Body Coordinate System (BCS)" to Console;
Console.CurrentTextColor = ColorTools.White;
Report @"To visualize Spacecraft's divergence from neutral LVLH state as it points towards the Sun.\n" to Console;

Report @"---------------------------------------------\n" to Console;




////	Color Scheme Used
//Report ColorTools.GetPaletteColors("Spectral");
//Console.CurrentTextColor = 255;
//Report "Spectral Color Palette" to Console;
//Console.CurrentTextColor = 32767;
//Report "Spectral Color Palette" to Console;
//Console.CurrentTextColor = 65535;
//Report "Spectral Color Palette" to Console;
//Console.CurrentTextColor = 65407;
//Report "Spectral Color Palette" to Console;
//Console.CurrentTextColor = 65280;
//Report "Spectral Color Palette" to Console;
//Console.CurrentTextColor = 8388352;
//Report "Spectral Color Palette" to Console;
//Console.CurrentTextColor = 16776960;
//Report "Spectral Color Palette" to Console;
//Console.CurrentTextColor = 16744192;
//Report "Spectral Color Palette" to Console;
//Console.CurrentTextColor = 16711680;
//Report "Spectral Color Palette" to Console;
//Console.CurrentTextColor = 16711807;
//Report "Spectral Color Palette" to Console;
//Console.CurrentTextColor = 16711935;
//Report "Spectral Color Palette" to Console;
//Console.CurrentTextColor = 8323327;
//Report "Spectral Color Palette" to Console;




//	//	LoggingInfo Console Setup

//	LoggingInfo Console Settings
LoggingInfo.DockMode	= 4;
LoggingInfo.Dimension 	= 36;
LoggingInfo.BackColor 	= ColorTools.Black;

LoggingInfo.CurrentFontType = 0;
LoggingInfo.WordWrap 		= 1;

LoggingInfo.WindowTitle	= "Logging Information";
LoggingInfo.Hide();



//Block


//	//	Display UserInterface

Show UserInterface1;




//	//	Configuration of Analysis Type

Console.CurrentTextColor = ColorTools.White;
Report @"~~~ Analysis Selection ~~~\n" to Console;

If (AnalysisType == 0.0);
	
	Console.CurrentTextColor = ColorTools.SpringGreen;
	Report @"\tAnalysis Type\t:" to Console without linefeed;
	Console.CurrentTextColor = ColorTools.White;
	Report @"\t Short Analysis" to Console;
	Console.CurrentTextColor = ColorTools.SpringGreen;
	Report @"\tNumber of Orbits\t:" to Console without linefeed;
	Console.CurrentTextColor = ColorTools.White;
	Report @"\t", OrbitToStop.Format("%2.1f%") to Console;
	
ElseIf (AnalysisType == 1.0);
	
	Console.CurrentTextColor = ColorTools.SpringGreen;
	Report @"\tAnalysis Type\t:" to Console without linefeed;
	Console.CurrentTextColor = ColorTools.White;
	Report @"\t Long Analysis"to Console;
	Console.CurrentTextColor = ColorTools.SpringGreen;
	Report @"\tNumber of Orbits\t:" to Console without linefeed;
	Console.CurrentTextColor = ColorTools.White;
	Report @"\tFull Lifetime" to Console;
	
End;

Report "" to Console;




//	//	Configuration of ThreeDModel

//	Setting Up of Objects
//		Note: ThreeDModel object is not available via the Object Browser
ThreeDModel tdmDS;		
ThreeDModel tdmSS;	
ThreeDModel tdmSSFP;	
ThreeDModel tdmSSRP;

StringArray TreeContentsDS;
StringArray TreeContentsSS;
StringArray TreeContentsSSFP;
StringArray TreeContentsSSRP;

Variable i;

String MainBody = "Main_Body";
String SolPanel = "Solar_Arrays";

Variable Scale = 100;

//	Importing of ThreeDModel
tdmDS.ModelFilename   = ThreeDModel_Filename;
tdmSS.ModelFilename   = ThreeDModel_Filename;
tdmSSFP.ModelFilename = ThreeDModel_Filename;
tdmSSRP.ModelFilename = ThreeDModel_Filename;

Console.CurrentTextColor = ColorTools.White;
Report @"~~~ ThreeDModel Selection ~~~\n" to Console;
Console.CurrentTextColor = ColorTools.Wheat;
Report @"\tThreeDModel Filename\t:" to Console;
Console.CurrentTextColor = ColorTools.White;
Report @"\t\t\t\t", ThreeDModel_Filename, @"\n" to Console;


//	Components of ThreeDModel
//		Note: Based on object name in Blender
tdmDS.ReportGroupTree(TreeContentsDS);
tdmSS.ReportGroupTree(TreeContentsSS);
tdmSSFP.ReportGroupTree(TreeContentsSSFP);
tdmSSRP.ReportGroupTree(TreeContentsSSRP);

TreeContentsDS.SortAscending();	
TreeContentsSS.SortAscending();	
TreeContentsSSFP.SortAscending();	
TreeContentsSSRP.SortAscending();	

Console.CurrentTextColor = ColorTools.Wheat;
