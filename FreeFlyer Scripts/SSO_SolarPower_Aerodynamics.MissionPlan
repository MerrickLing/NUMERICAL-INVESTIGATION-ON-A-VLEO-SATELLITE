        <ScriptRegion enabled="true" collapsed="false" StartLineComment="Slewing of Spacecrafts">
          <FreeForm enabled="true">
            <Label>Slewing for DummySat</Label>
            <Bookmarks />
            <Breakpoints />
            <FreeFormScript><![CDATA[
//	//	Slewing for DummySat

//	Slew to LVLH Coordinate System - Earth Pointing 
Call ComputeTargetAttitude(DummySat, Canberra, targetCS_DS, 0);
Call SlewSpacecraft(targetCS_DS, PERFORM_SLEW, SLEW_RATE, DummySat);


//	1A: For START of Contact with the GroundStations (first in sequence)/ the Sun (last in sequence)
If (GrndStnPointing == 1.0);
	
	If (toCanberra_DS.Visibility(DummySat.Epoch) == 1 and inContactCanberra_DS == 0);	//	Canberra: Visible, but NOT in Contact
	    inContactCanberra_DS = 1;														//	Update to be in Contact
	    dsCanberra.Active = 1;															//	Update Display to Contact

		LoggingInfo.CurrentTextColor = ColorTools.Red;
	    Report "DS: Contact with Canberra has begun..." to LoggingInfo;
		toCanberra_DS_StartEpoch = DummySat.Epoch;
		Report toCanberra_DS_StartEpoch.ConvertToCalendarDate() to LoggingInfo;
	End;

	If (toGoldstone_DS.Visibility(DummySat.Epoch) == 1 and inContactGoldstone_DS == 0);		//	Goldstone: Visible, but NOT in Contact
	    inContactGoldstone_DS = 1;															//	Update to be in Contact
	    dsGoldstone.Active = 1;																//	Update Display to Contact

		LoggingInfo.CurrentTextColor = ColorTools.DarkViolet;
	    Report "DS: Contact with Goldstone has begun..." to LoggingInfo;
		toGoldstone_DS_StartEpoch = DummySat.Epoch;
		Report toGoldstone_DS_StartEpoch.ConvertToCalendarDate() to LoggingInfo;
	End;

	If (toMadrid_DS.Visibility(DummySat.Epoch) == 1 and inContactMadrid_DS == 0);	//	Madrid: Visible, but NOT in Contact
	    inContactMadrid_DS = 1;														//	Update to be in Contact
	    dsMadrid.Active = 1;														//	Update Display to Contact

		LoggingInfo.CurrentTextColor = ColorTools.Aqua;
	    Report "DS: Contact with Madrid has begun..." to LoggingInfo;
		toMadrid_DS_StartEpoch = DummySat.Epoch;
		Report toMadrid_DS_StartEpoch.ConvertToCalendarDate() to LoggingInfo;
	End; 
	
End;

If (toSun_DS.Visibility(DummySat.Epoch) == 1 and inSunlight_DS == 0);	//	Sun: Visible, but NOT in Contact
    inSunlight_DS = 1;													//	Update to be in Sunlight
	dsSun.Active = 1;													//	Update Display to Contact
	
	LoggingInfo.CurrentTextColor = 8388352;
	Report "DS: Charging has started..." to LoggingInfo;
	toSun_DS_StartEpoch = DummySat.Epoch;
	Report toSun_DS_StartEpoch.ConvertToCalendarDate() to LoggingInfo;
End;


//	1B: For END of Contact with the GroundStations/ the Sun	
If (GrndStnPointing == 1.0);
	
	If (toCanberra_DS.Visibility(DummySat.Epoch) == 0 and inContactCanberra_DS == 1);	//	Canberra: NOT Visible, but in Contact
	    inContactCanberra_DS = 0;														//	Update to NOT be in Contact
		dsCanberra.Active = 0;															//	Update Display to NO Contact

	    LoggingInfo.CurrentTextColor = ColorTools.Red;
	    Report "DS: Contact with Canberra has ended..." to LoggingInfo;
		toCanberra_DS_EndEpoch = DummySat.Epoch;
		Report toCanberra_DS_EndEpoch.ConvertToCalendarDate() to LoggingInfo;
		Report @"Contact Time\t: ", (toCanberra_DS_EndEpoch - toCanberra_DS_StartEpoch).ToMinutes.Format("%3.2f") to LoggingInfo;
	End;

	If (toGoldstone_DS.Visibility(DummySat.Epoch) == 0 and inContactGoldstone_DS == 1);		//	Goldstone: NOT Visible, but in Contact
	    inContactGoldstone_DS = 0;															//	Update to NOT be in Contact
		dsGoldstone.Active = 0;																//	Update Display to NO Contact

	    LoggingInfo.CurrentTextColor = ColorTools.DarkViolet;
	    Report "DS: Contact with Goldstone has ended..." to LoggingInfo;
		toGoldstone_DS_EndEpoch = DummySat.Epoch;
		Report toGoldstone_DS_EndEpoch.ConvertToCalendarDate() to LoggingInfo;
		Report @"Contact Time\t: ", (toGoldstone_DS_EndEpoch - toGoldstone_DS_StartEpoch).ToMinutes.Format("%3.2f") to LoggingInfo;
	End;

	If (toMadrid_DS.Visibility(DummySat.Epoch) == 0 and inContactMadrid_DS == 1);	//	Madrid: NOT Visible, but in Contact
	    inContactMadrid_DS = 0;														//	Update to NOT be in Contact
		dsMadrid.Active = 0;														//	Update Display to NO Contact

	    LoggingInfo.CurrentTextColor = ColorTools.Aqua;
	    Report "DS: Contact with Madrid has ended..." to LoggingInfo;
		toMadrid_DS_EndEpoch = DummySat.Epoch;
		Report toMadrid_DS_EndEpoch.ConvertToCalendarDate() to LoggingInfo;
		Report @"Contact Time\t: ", (toMadrid_DS_EndEpoch - toMadrid_DS_StartEpoch).ToMinutes.Format("%3.2f") to LoggingInfo;
	End;

End;

If (toSun_DS.Visibility(DummySat.Epoch) == 0 and inSunlight_DS == 1);	//	Sun: NOT Visible, but in Contact
    inSunlight_DS = 0;													//	Update to NOT be in Sunlight
	dsSun.Active = 0;													//	Update Display to NO Contact
	
	LoggingInfo.CurrentTextColor = 8323327;
	Report "DS: Charging has stopped... " to LoggingInfo;
